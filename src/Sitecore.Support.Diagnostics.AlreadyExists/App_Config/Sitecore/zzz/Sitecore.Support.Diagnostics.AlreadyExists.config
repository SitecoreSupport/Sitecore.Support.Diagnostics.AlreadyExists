<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery">
    <dataAdapterManager defaultProvider="xconnect">
      <providers>
        <add name="xconnect" type="Sitecore.Analytics.XConnect.DataAccess.XConnectDataAdapterProvider, Sitecore.Analytics.XConnect">
          <patch:attribute name ="type">Sitecore.Support.Analytics.XConnect.DataAccess.XConnectDataAdapterProvider, Sitecore.Support.Diagnostics.AlreadyExists</patch:attribute>
        </add>
      </providers>
    </dataAdapterManager>
  </sitecore>
</configuration>